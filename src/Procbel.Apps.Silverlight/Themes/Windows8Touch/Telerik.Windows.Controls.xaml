              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <chromes:ButtonChrome RenderEnabled="{TemplateBinding IsEnabled}" RenderMouseOver="{TemplateBinding IsMouseOver}" RenderPressed="{TemplateBinding IsPressed}" />
            <ContentControl x:Name="Arrow" Template="{StaticResource ScrollArrow_Right}" Foreground="{StaticResource ButtonIconForeground_Normal}" Background="{StaticResource ButtonIconBackground_Normal}" />
          </Grid>
        </ControlTemplate>
      </Setter.Value>
