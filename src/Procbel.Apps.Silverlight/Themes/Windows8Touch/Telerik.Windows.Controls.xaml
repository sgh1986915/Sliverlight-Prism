  <ResourceDictionary.MergedDictionaries>
    <telerik:Windows8TouchResourceDictionary />
  </ResourceDictionary.MergedDictionaries>
  <!-- Brushes-->
  <telerik:Windows8TouchColors x:Key="Windows8TouchColors" />
  <SolidColorBrush x:Key="ButtonIconForeground_MouseOver" Color="#FF282828" />
  <SolidColorBrush x:Key="ButtonIconBackground_MouseOver" Color="Transparent" />
  <SolidColorBrush x:Key="ButtonIconForeground_Pressed" Color="#FF282828" />
  <SolidColorBrush x:Key="ButtonIconBackground_Pressed" Color="Transparent" />
  <SolidColorBrush x:Key="ButtonIconForeground_Disabled" Color="#FF282828" />
  <SolidColorBrush x:Key="ButtonIconBackground_Disabled" Color="Transparent" />
  <ControlTemplate x:Key="ScrollArrow_Right" TargetType="ContentControl">
    <Grid>
      <Path x:Name="BackgroundIcon" Data="M0,0 L0,2 1,1 Z" Stretch="Fill" Width="4" Height="7" Fill="{TemplateBinding Background}" Margin="0 2 0 0" />
      <Path x:Name="ForegroundIcon" Data="M0,0 L0,2 1,1 Z" Stretch="Fill" Width="4" Height="7" Fill="{TemplateBinding Foreground}" Margin="0 1 0 1" />
    </Grid>
  </ControlTemplate>
  <SolidColorBrush x:Key="ButtonIconForeground_Normal" Color="#FF282828" />
  <SolidColorBrush x:Key="ButtonIconBackground_Normal" Color="Transparent" />
  <ControlTemplate x:Key="ScrollArrow_Left" TargetType="ContentControl">
    <Grid>
      <Path x:Name="BackgroundIcon" Data="M1,0 L0,1 1,2 Z" Stretch="Fill" Width="4" Height="7" Fill="{TemplateBinding Background}" Margin="0 2 0 0" />
      <Path x:Name="ForegroundIcon" Data="M1,0 L0,1 1,2 Z" Stretch="Fill" Width="4" Height="7" Fill="{TemplateBinding Foreground}" Margin="0 1 0 1" />
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="ScrollArrow_Down" TargetType="ContentControl">
    <Grid>
      <Path x:Name="BackgroundIcon" Data="M0,0 L2,0 1,1 Z" Stretch="Fill" Width="7" Height="4" Fill="{TemplateBinding Background}" Margin="0 2 0 0" />
      <Path x:Name="ForegroundIcon" Data="M0,0 L2,0 1,1 Z" Stretch="Fill" Width="7" Height="4" Fill="{TemplateBinding Foreground}" Margin="0 1 0 1" />
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="ScrollArrow_Up" TargetType="ContentControl">
    <Grid>
      <Path x:Name="BackgroundIcon" Data="M0,1 L1,0 2,1 Z" Stretch="Fill" Width="7" Height="4" Fill="{TemplateBinding Background}" Margin="0 2 0 0" />
      <Path x:Name="ForegroundIcon" Data="M0,1 L1,0 2,1 Z" Stretch="Fill" Width="7" Height="4" Fill="{TemplateBinding Foreground}" Margin="0 1 0 1" />
    </Grid>
  </ControlTemplate>
  <SolidColorBrush x:Key="ScrollBarThumbGripFill_MouseOver" Color="#FF3C7FB1" />
  <SolidColorBrush x:Key="ScrollBarThumbGripStroke_MouseOver" Color="#FFFFFFFF" />
  <SolidColorBrush x:Key="ScrollBarThumbGripFill_Disabled" Color="#FF898C95" />
  <SolidColorBrush x:Key="ScrollBarThumbGripStroke_Disabled" Color="#FFFFFFFF" />
  <ControlTemplate x:Key="ScrollBarThumbGripHorizontal" TargetType="ContentControl">
    <Grid>
      <Rectangle Fill="{TemplateBinding Foreground}" Width="6" Height="9" />
      <Path Stroke="{TemplateBinding Background}" Data="M0.5,0 L0.5,9 M2.5,0 L2.5,9 M4.5,0 L4.5,9" Width="6" Height="9" />
    </Grid>
  </ControlTemplate>
  <SolidColorBrush x:Key="ScrollBarThumbGripFill_Normal" Color="#FF898C95" />
  <SolidColorBrush x:Key="ScrollBarThumbGripStroke_Normal" Color="#FFFFFFFF" />
  <ControlTemplate x:Key="ScrollBarThumbGripVertical" TargetType="ContentControl">
    <Grid>
      <Rectangle Fill="{TemplateBinding Foreground}" Width="9" Height="6" />
      <Path Stroke="{TemplateBinding Background}" Data="M0,0.5 L9,0.5 M0,2.5 L9,2.5 M0,4.5 L9,4.5" Width="9" Height="6" />
    </Grid>
  </ControlTemplate>
  <SolidColorBrush x:Key="ControlOuterBorder_Normal" Color="#FF898C95" />
  <Color x:Key="ControlBackground_Normal_Stop0">#FFF2F2F2</Color>
  <SolidColorBrush x:Key="ControlInnerBorder_Normal" Color="#FFFCFCFC" />
  <Color x:Key="ControlBackground_Normal_Stop3">#FFD0D0D0</Color>
  <LinearGradientBrush x:Key="RangeControlBackground_Normal" StartPoint="0.5,0" EndPoint="0.5,1">
    <GradientStop Color="#FFDAE1E4" Offset="0.033" />
    <GradientStop Color="#FFFAFAFA" Offset="0.586" />
  </LinearGradientBrush>
  <LinearGradientBrush x:Key="RangeControlBackgroundVertical_Normal" EndPoint="1,0.5" StartPoint="0,0.5">
    <GradientStop Color="#FFDAE1E4" Offset="0.033" />
    <GradientStop Color="#FFFAFAFA" Offset="0.586" />
  </LinearGradientBrush>
  <LinearGradientBrush x:Key="DragDrop_VisualCueBackground" EndPoint="0.5,0" StartPoint="0.5,1">
    <GradientStop Color="#FFF1F8FD" Offset="0" />
    <GradientStop Color="#FFD5EFFC" Offset="1" />
  </LinearGradientBrush>
  <SolidColorBrush x:Key="DragDrop_VisualCueBorder" Color="#FFA6D9F4" />
  <SolidColorBrush x:Key="MainBrush" Color="{Binding Path=Palette.MainColor, Source={StaticResource Windows8TouchColors}}" telerik:Windows8TouchPalette.IsFreezable="True" />
  <SolidColorBrush x:Key="MediumBrush" Color="{Binding Path=Palette.MediumColor, Source={StaticResource Windows8TouchColors}}" telerik:Windows8TouchPalette.IsFreezable="True" />
  <SolidColorBrush x:Key="ControlForeground_Normal" Color="#FF000000" />
  <LinearGradientBrush EndPoint="0,1" x:Key="FocusBrush">
