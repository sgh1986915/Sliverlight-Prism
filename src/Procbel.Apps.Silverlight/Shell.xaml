    	xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
	xmlns:local="clr-namespace:Procbel.Apps.Silverlight"             
    xmlns:viewModels="clr-namespace:Procbel.Apps.Silverlight.ViewModels"         
             
