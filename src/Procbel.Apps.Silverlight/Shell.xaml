                                    <RotateTransform/>
                                    <TranslateTransform X="630"/>
                                </TransformGroup>
                            </StackPanel.RenderTransform>
                            <TextBlock TextWrapping="Wrap" Text="Download Demo"  FontSize="22" HorizontalAlignment="Left" Foreground="{StaticResource text}" Margin="0,0,0,20" Opacity="0.8"/>
                            <TextBlock TextWrapping="Wrap"  Foreground="{StaticResource text}" Opacity="0.6" FontSize="12" Margin="2,0,0,0" Text="Download the source code and see how this demo was created.  Learn about best practices used and explore the custom styling."/>
                            <TextBlock TextWrapping="Wrap"  Foreground="{StaticResource text}" Opacity="1" FontSize="12" Margin="2,10,0,0" Text="*The source code will be available for download with the Q2 Beta release." Visibility="Collapsed"/>
                            <!--    						<telerik:RadButton Content="Download" Style="{StaticResource RadButtonStyle}" Foreground="{StaticResource text}" BorderBrush="{StaticResource text}" HorizontalAlignment="Center" Padding="20,3" Margin="0,10,0,0" Width="164"
							Click="OnDownloadButtonClick"/> -->
                        </StackPanel>
                    </Grid>
                </Grid>
            </Grid>


