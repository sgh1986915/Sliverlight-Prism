                <ResourceDictionary Source="/Procbel.Apps.Silverlight.Theme;component/Styles/RadButtonStyle.xaml"/>
                <!--<ResourceDictionary Source="Styles/RadButtonStyle.xaml"/>-->
            </ResourceDictionary.MergedDictionaries>
            <Storyboard x:Name="InitialAnimation" x:Key="InitialAnimation">
