                                        </Border>
                                        <Border BorderThickness="0" Margin="0,0,20,9" Height="17" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="10">
                                            <Border.Background>
                                                <SolidColorBrush Color="{Binding clothingcolor}"/>
                                            </Border.Background>
                                        </Border>
                                        <Border BorderThickness="0" Margin="0,0,8,9" Height="10" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="10">
                                            <Border.Background>
                                                <SolidColorBrush Color="{Binding accessoriescolor}"/>
                                            </Border.Background>
                                        </Border>
                                        <TextBlock TextWrapping="Wrap" Text="{Binding Name}" 
											FontSize="12" Margin="9,5,0,0" VerticalAlignment="Top">
                                            <TextBlock.Foreground>
                                                <SolidColorBrush Color="{Binding textcolor}"/>
                                            </TextBlock.Foreground>
                                        </TextBlock>
                                    </Grid>
                                </telerik:RadRadioButton>
                            </ItemsControl>
