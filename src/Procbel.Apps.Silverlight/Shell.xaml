    <!-- FontFamily="Segoe UI"-->
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <app:ApplicationResources x:Key="ApplicationResources" />
                </ResourceDictionary>
               
