                                    <Grid>
                                        <Border BorderThickness="1">
                                            <Border.Background>
                                                <RadialGradientBrush GradientOrigin="0,0">
                                                    <GradientStop Color="{Binding dashboardcolor1}" Offset="0.992"/>
                                                    <GradientStop Color="{Binding dashboardcolor2}"/>
                                                </RadialGradientBrush>
                                            </Border.Background>
                                        </Border>
                                        <Border BorderThickness="0" Margin="10,0,0,9" Height="14" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="10">
                                            <Border.Background>
                                                <SolidColorBrush Color="{Binding bikescolor}"/>
                                            </Border.Background>
                                        </Border>
                                        <Border BorderThickness="0" Margin="22,30,0,9" HorizontalAlignment="Left" Width="10">
                                            <Border.Background>
                                                <SolidColorBrush Color="{Binding componenetscolor}"/>
