                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="ContentRegionPlaceholder">
                    <DiscreteDoubleKeyFrame KeyTime="0:0:5.5" Value="1"/>
                </DoubleAnimationUsingKeyFrames>

                
                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(TextBlock.Text)" Storyboard.TargetName="Title">
                    <DiscreteObjectKeyFrame KeyTime="0:0:6" Value="Procbel Apps"/>
                    <DiscreteObjectKeyFrame KeyTime="0:0:7" Value="{Binding Strings.ApplicationName, Source={StaticResource ApplicationResources}}"/>
                    
                </ObjectAnimationUsingKeyFrames>

